page {
  height: 100%;
  width: 100%;
  font-size: 14px;
}
.page-bg {
  width: 100%;
  background: #f2f2f2;
}
.page-bg .search-view {
  background: #fff;
  width: 100%;
  display: flex;
  align-items: center;
  box-shadow: 0px 2px 8px 0 #c8c8c8;
}
.page-bg .search-view .search-field {
  flex: 1;
}
.page-bg .search-view .lines {
  width: 2rpx;
  height: 22px;
  background: #eee;
}
.page-bg .search-view .cancel-button {
  color: #333;
  margin-right: 20rpx;
  padding: 10rpx 30rpx;
}
.page-bg .pois-list-bg {
  margin-top: 20rpx;
  background: #fff;
  width: calc(100% - 20px);
  border-radius: 8rpx;
  position: absolute;
  bottom: 0;
  left: 10px;
  overflow: hidden;
  top: 44px;
}
.page-bg .pois-list-bg .text-info-bg {
  width: 100%;
}
.page-bg .pois-list-bg .cell-bg {
  display: flex;
  align-items: center;
}
