.card-item-box {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  width: 100%;
  height: 100%;
}
.card-item-box .card-item-title {
  color: #333;
  font-size: 14px;
  display: inline-block;
}
.card-item-box .card-item-img {
  display: block;
  height: 90rpx;
  width: 90rpx;
  border-radius: 45rpx;
}
